command: '!clearmulti'
message: |
    edit !multi $(twitch $(channel) "{{displayName}}") is not currently streaming with anyone else, but thank you for checking.
userlevel: 'moderator'
alias: '!commands'
cooldown: 30
usage: '!clearmulti'
description: |
    Clear a previously set !multi command
sample: |
    @DarthMinos -> The command "!multi" has been edited successfully.
required:
- !multi/[default]
- !clearmulti/[default]