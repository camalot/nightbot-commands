command: |
    !so
message: |
    $(twitch $(eval "$(touser)".replace(/^\@/gi,"")) "Give {{displayName}} a follow.") - https://twitch.tv/$(eval "$(touser)".replace(/^\@/gi,"");)
userlevel:
    moderator
cooldown:
    30
alias:
description: |
    Shout out a user to put their channel link in the chat.
usage: |
    !so @DarthMinos
sample: |
   Give DarthMinos a follow. <a href="https://twitch.tv/darthminos">https://twitch.tv/darthminos</a>