command: '!retweet'
message: |
    No tweet is currently set. But you can tweet the stream $(tweet Come watch $(channel) $(twitch $(channel) "{{game}} on Twitch! {{url}}"))
userlevel: 'everyone'
cooldown: 30
alias:
    
usage: '!retweet'
description: |
    Displays the currently set tweet url. Set from !settweet.
sample: |
    Want to help promote the stream? Retweet the stream tweet. https://twitter.com/abryanconrad/status/1079619797564968960
required:
- '!cleartweet/[default]'
- '!settweet/[default]'