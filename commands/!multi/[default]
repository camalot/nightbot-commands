command: '!multi'
message: |
    $(twitch $(channel) "{{displayName}}") is not currently streaming with anyone else, but thank you for checking.
userlevel: 'everyone'
cooldown: 30
alias:
usage: '!multi'
description: |
    Displays the current multi stream link
sample: |
    DarthMinos is streaming with thekrymzun, sabborn : Watch from everyone's perspective here: https://multi-stream.tv/twitch,darthminos/twitch,thekrymzun/twitch,sabborn
required:
- '!setmulti/[default]'
- '!clearmulti/[default]'