command: '!unlurk'
message: |
    $(twitch $(user) "{{displayName}}") has emerged from the shadows. 
description:
    A message for the user to notify they are coming out of lurk mode.
userlevel: 'everyone'
usage: '!unlurk'
cooldown: 10
alias:
optional:
- '!lurk/[default]'
- '!relurk/[default]'
sample: |
    DarthMinos has emerged from the shadows.
