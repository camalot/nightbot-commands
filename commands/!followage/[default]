command: '!followage'
message: |
    $(touser) Has Been Following $(channel) On Twitch Since $(customapi http://api.newtimenow.com/follow-length/?channel=$(channel)&user=$(eval "$(touser)".replace(/\@/gi,"");))
userlevel: 'everyone'
cooldown: 30
alias:
usage: '!followage @DarthMinos'
description: |
    Says when the user followed the channel.