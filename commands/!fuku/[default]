command: '!fuku'
message: |
    $(eval fu = JSON.parse(`$(urlfetch http://fuku.io/api/random)`); (`${fu.text} (${fu.language}) ${fu.help ? " - " + fu.help : "" } - http://fuku.io/${fu.code}`))
userlevel: 'everyone'
cooldown: 30
alias:
usage: '!fuku'
description: |
    uses fuku.io to display 'Fuck You' in the specified language in chat. 