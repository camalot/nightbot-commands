command: '!wins'
message: |
    $(twitch $(channel) "{{displayName}}") has 0 wins.
userlevel: 'everyone'
alias: ''
cooldown: 30
usage: '!wins'
description: |
    Displays the total number of wins that have been set by the <code>!addwin</code> command.
sample: |
    @DarthMinos has 30 wins.
required:
- '!addwin/[default]'